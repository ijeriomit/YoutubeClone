{"ast":null,"code":"import PageForm from \"./components/PageForm.vue\";\nexport default {\n  name: \"App\",\n  components: {\n    PageForm\n  },\n  data: function () {\n    return {\n      clickCount: 0\n    };\n  },\n  methods: {\n    async updateButtonClick() {\n      this.clickCount += 1;\n      try {\n        const response = await fetch(\"http://localhost:8383/counter\", {\n          method: \"POST\",\n          headers: {\n            \"Content-Type\": \"application/json\"\n          },\n          body: JSON.stringify({\n            counter: this.clickCount\n          })\n        });\n        console.log(response.status);\n      } catch (e) {\n        console.error(\"problem sending counter\", e);\n      }\n    }\n  }\n};","map":{"version":3,"names":["PageForm","name","components","data","clickCount","methods","updateButtonClick","response","fetch","method","headers","body","JSON","stringify","counter","console","log","status","e","error"],"sources":["/Users/ijeriomitogun/Projects/TypescriptProjects/upstart-interview/web-client/src/App.vue"],"sourcesContent":["<template>\n  <img alt=\"Vue logo\" src=\"./assets/logo.png\" />\n  <PageForm></PageForm>\n  <button @click=\"updateButtonClick()\">Click Me</button>\n</template>\n\n<script>\nimport PageForm from \"./components/PageForm.vue\";\n\nexport default {\n  name: \"App\",\n  components: {\n    PageForm,\n  },\n  data: function () {\n    return {\n      clickCount: 0,\n    };\n  },\n  methods: {\n    async updateButtonClick() {\n      this.clickCount += 1;\n      try {\n        const response = await fetch(\"http://localhost:8383/counter\", {\n          method: \"POST\",\n          headers: {\n            \"Content-Type\": \"application/json\",\n          },\n          body: JSON.stringify({ counter: this.clickCount }),\n        });\n        console.log(response.status);\n      } catch (e) {\n        console.error(\"problem sending counter\", e);\n      }\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n#a